{
  "version": 2,
  "name": "streamflow-player",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
            "key": "X-Frame-Options",
            "value": "DENY"
        }
      ]
    }
  ]
}